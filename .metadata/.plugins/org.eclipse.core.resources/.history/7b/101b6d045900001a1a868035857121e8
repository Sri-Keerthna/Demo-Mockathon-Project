package com.hcl.trade.exceptions;

import org.springframework.http.HttpStatus;
import org.springframework.web.bind.annotation.ResponseStatus;

@ResponseStatus(value=HttpStatus.NOT_FOUND, reason="StockId Not Found") 
public class StockIdNotFoundException extends Exception{

	private static final long serialVersionUID = -3332292346834265371L;

	public StockIdNotFoundException(int stockid){
		super("StockIdNotFoundException with id="+stockid);
	}
}
